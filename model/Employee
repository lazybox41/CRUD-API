const mongoose = require("mongoose");

const Employee = mongoose.model("Employee", {
    name: {
        type: String,
        required: true,
    },
    empID: {
        type: Number,
        required: true,
    },
    location: {
        type: String,
        required: true,
    },
    salary: {
        type: String,
        required: true,
    },
});

module.exports = Employee;
